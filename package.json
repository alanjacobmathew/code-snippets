{
    "name": "code-snippets",
    "description": "Manage code snippets running on a WordPress-powered site through a graphical interface.",
    "homepage": "https://wordpress.org/plugins/code-snippets",
    "version": "3.6.5.1",
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "test": "eslint && npm run phpcs",
        "build": "webpack --mode development",
        "watch": "webpack --mode development --watch",
        "bundle": "ts-node scripts/bundle.ts",
        "phpcs": "vendor/bin/phpcs -s --colors ./phpcs.xml",
        "phpcbf": "vendor/bin/phpcbf ./phpcs.xml",
        "version": "ts-node scripts/version.ts",
        "version-dev": "npm version --git-tag-version=false --preid=dev",
        "version-alpha": "npm version --git-tag-version=false --preid=alpha",
        "version-beta": "npm version --git-tag-version=false --preid=beta",
        "version-rc": "npm version --git-tag-version=false --preid=rc"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/codesnippetspro/code-snippets.git"
    },
    "keywords": [
        "wordpress-plugin"
    ],
    "author": {
        "name": "Shea Bunge",
        "email": "shea@codesnippets.pro",
        "url": "https://sheabunge.com"
    },
    "license": "GPL-2.0-or-later",
    "bugs": {
        "url": "https://github.com/codesnippetspro/code-snippets/issues"
    },
    "dependencies": {
        "@codemirror/fold": "^0.19.3",
        "@jcubic/tagger": "^0.4.5",
        "@wordpress/api-fetch": "^6.33.0",
        "@wordpress/block-editor": "^12.3.0",
        "@wordpress/blocks": "^12.12.0",
        "@wordpress/components": "^25.1.0",
        "@wordpress/data": "^9.5.0",
        "@wordpress/dom-ready": "^3.36.0",
        "@wordpress/i18n": "^4.36.0",
        "@wordpress/icons": "^9.26.0",
        "@wordpress/server-side-render": "^4.12.0",
        "axios": "^1.4.0",
        "classnames": "^2.3.2",
        "codemirror": "^5.29",
        "codemirror-colorpicker": "^1.9.80",
        "php-parser": "github:glayzzle/php-parser#php8",
        "prismjs": "^1.29.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-select": "^5.7.3"
    },
    "devDependencies": {
        "@eslint/js": "^9.9.0",
        "@tsconfig/node18": "^18.2.4",
        "@types/archiver": "^6.0.2",
        "@types/codemirror": "^5.60.8",
        "@types/jquery": "^3.5.16",
        "@types/node": "^20.14.15",
        "@types/prismjs": "^1.26.0",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@types/rtlcss": "^3.5.1",
        "@types/tinymce": "^4.6.5",
        "@types/web": "^0.0.102",
        "@types/wordpress__editor": "^13.6.2",
        "@typescript-eslint/eslint-plugin": "^5.60.1",
        "@typescript-eslint/parser": "^5.60.1",
        "@wordpress/babel-preset-default": "^7.20.0",
        "@wordpress/eslint-plugin": "^14.8.0",
        "archiver": "^7.0.1",
        "autoprefixer": "^10.4.14",
        "babel-loader": "^9.1.2",
        "babel-plugin-prismjs": "^2.1.0",
        "css-loader": "^7.1.2",
        "cssnano": "^6.0.1",
        "eslint": "^8.57.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.9",
        "eslint-webpack-plugin": "^4.2.0",
        "glob": "^11.0.0",
        "mini-css-extract-plugin": "^2.9.0",
        "postcss": "^8.4.24",
        "postcss-hexrgba": "^2.1.0",
        "postcss-load-config": "^6.0.1",
        "postcss-loader": "^8.1.1",
        "sass": "^1.63.6",
        "sass-loader": "^16.0.0",
        "style-loader": "^4.0.0",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.5.4",
        "webpack": "^5.88.1",
        "webpack-cli": "^5.1.4",
        "webpack-merge": "^6.0.1",
        "webpack-remove-empty-scripts": "^1.0.4",
        "webpack-rtl-plugin": "^2.0.0"
    }
}
