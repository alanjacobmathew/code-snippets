{
    "name": "code-snippets",
    "description": "Manage code snippets running on a WordPress-powered site through a graphical interface.",
    "homepage": "https://wordpress.org/plugins/code-snippets",
    "version": "3.6.5.1",
    "main": "src/dist/edit.js",
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "test": "eslint && npm run phpcs",
        "build": "webpack",
        "watch": "webpack --watch",
        "bundle": "ts-node scripts/bundle.ts",
        "phpcs": "src/vendor/bin/phpcs -s --colors ./phpcs.xml",
        "phpcbf": "src/vendor/bin/phpcbf ./phpcs.xml",
        "version": "ts-node scripts/version.ts",
        "version-dev": "npm version --git-tag-version=false --preid=dev",
        "version-alpha": "npm version --git-tag-version=false --preid=alpha",
        "version-beta": "npm version --git-tag-version=false --preid=beta",
        "version-rc": "npm version --git-tag-version=false --preid=rc"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/codesnippetspro/code-snippets.git"
    },
    "keywords": [
        "wordpress-plugin"
    ],
    "author": "Shea Bunge <shea@codesnippets.pro> (https://sheabunge.com)",
    "license": "GPL-2.0-or-later",
    "bugs": {
        "url": "https://github.com/codesnippetspro/code-snippets/issues"
    },
    "dependencies": {
        "@codemirror/fold": "^0.19.4",
        "@jcubic/tagger": "^0.6.2",
        "@wordpress/api-fetch": "^7.5.0",
        "@wordpress/block-editor": "^14.0.0",
        "@wordpress/blocks": "^13.5.0",
        "@wordpress/components": "^28.5.0",
        "@wordpress/data": "^10.5.0",
        "@wordpress/dom-ready": "^4.5.0",
        "@wordpress/i18n": "^5.5.0",
        "@wordpress/icons": "^10.5.0",
        "@wordpress/server-side-render": "^5.5.0",
        "axios": "^1.7.4",
        "classnames": "^2.5.1",
        "codemirror": "^5.29",
        "codemirror-colorpicker": "^1.9.80",
        "php-parser": "^3.1.5",
        "prismjs": "^1.29.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-select": "^5.8.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.9.0",
        "@stylistic/eslint-plugin": "^2.6.4",
        "@tsconfig/node18": "^18.2.4",
        "@types/archiver": "^6.0.2",
        "@types/codemirror": "^5.60.15",
        "@types/jquery": "^3.5.30",
        "@types/node": "^22.3.0",
        "@types/prismjs": "^1.26.4",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@types/rtlcss": "^3.5.4",
        "@types/tinymce": "^4.6.9",
        "@types/web": "^0.0.155",
        "@types/wordpress__editor": "^13.6.8",
        "@typescript-eslint/eslint-plugin": "^8.1.0",
        "@typescript-eslint/parser": "^8.1.0",
        "@wordpress/babel-preset-default": "^8.5.0",
        "archiver": "^7.0.1",
        "autoprefixer": "^10.4.20",
        "babel-loader": "^9.1.3",
        "babel-plugin-prismjs": "^2.1.0",
        "css-loader": "^7.1.2",
        "cssnano": "^7.0.5",
        "eslint": "^9.9.0",
        "eslint-plugin-react": "^7.35.0",
        "eslint-plugin-react-refresh": "^0.4.9",
        "eslint-webpack-plugin": "^4.2.0",
        "glob": "^11.0.0",
        "globals": "^15.9.0",
        "mini-css-extract-plugin": "^2.9.0",
        "postcss": "^8.4.41",
        "postcss-hexrgba": "^2.1.0",
        "postcss-load-config": "^6.0.1",
        "postcss-loader": "^8.1.1",
        "sass": "^1.77.8",
        "sass-loader": "^16.0.0",
        "style-loader": "^4.0.0",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.5.4",
        "typescript-eslint": "^8.1.0",
        "webpack": "^5.93.0",
        "webpack-cli": "^5.1.4",
        "webpack-merge": "^6.0.1",
        "webpack-remove-empty-scripts": "^1.0.4",
        "webpack-rtl-plugin": "^2.0.0"
    }
}
